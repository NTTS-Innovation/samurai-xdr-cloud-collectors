{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "IntegrationsId": {
        "type": "string",
        "metadata": {
          "description": "Integrations Id"
        }
      },
      "passkey": {
        "type": "securestring",
        "metadata": {
          "description": "Integrations Passkey"
        }
    },
      "endPoint": {
      "type": "string",
      "defaultValue": "api.westeurope.samurai.security.ntt",
      "metadata": {
          "description": "Provide the URL for the WebHook to receive events."
      },
      "allowedValues": [
          "api.westeurope.samurai.security.ntt",
          "api.westeurope.develop.samurai.security.ntt"
      ]
      }
    },
    "variables": {},
    "resources": [
      {
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2019-10-01",
        "name": "linkedTemplate",
        "properties": {
          "mode": "Incremental",
          "parameters": {
            "adminUsername": {
                "type": "string",
                "defaultValue": "ntt-samurai"
            },
            "adminPassword": {
                "type": "securestring",
                "metadata": {
                    "description": "Admin password on all VMs."
                }
            },
            "subnetId": {
                "type": "string"
            },
            "customData": {
                "type": "string"
            }
          },
          "templateLink": {
            "uri": "https://raw.githubusercontent.com/NTTS-Innovation/samurai-xdr-cloud-integrations/main/providers/azure/cloudCollectorDeploy.json",
            "contentVersion": "1.0.0.0"
          }
        }
      }
    ]
  }